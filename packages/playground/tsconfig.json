{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es2015",
    "lib": [ "dom", "dom.iterable", "esnext" ],
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": "./"
    // "paths": {
    //     "@/*": [
    //         "./src/*"
    //     ],
    //     "@styles/*": [
    //         "./src/styling/scss/*"
    //     ],
    //     "@model/*": [
    //         "./src/model/*"
    //     ],
    //     "@components/*": [
    //         "./src/components/*"
    //     ]
    // },
  },
    "include": [
        "./src/**/*",
        "**/*.ts",
        "**/*.tsx",
    ],
    "exclude": [
        "node_modules",
    ],
}