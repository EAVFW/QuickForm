{
    "layout": {
        "autoAdvanceSlides": false,
        "defaultSlideButtonIcon": "ChevronRight",
        "slides": {
            "slide1": {
                "title": "Beregn prisen for rengøring af fliser",
                "buttonText": "Næste",
                "rows": {
                    "row1": {
                        "columns": {
                            "col1": {
                                "rows": {
                                    "row1_1": {
                                        "type": "question",
                                        "ref": "textTest"
                                    },
                                    "row1_2": {
                                        "type": "question",
                                        "ref": "multilineTest"
                                    },
                                    "row1_3": {
                                        "type": "question",
                                        "ref": "emailTest"
                                    },
                                    "row1_4": {
                                        "type": "question",
                                        "ref": "phoneTest"
                                    },
                                    "row1_5": {
                                        "type": "question",
                                        "ref": "sliderTest"
                                    },
                                    "row1_6": {
                                        "type": "question",
                                        "ref": "radioTest"
                                    },
                                    "row1_7": {
                                        "type": "question",
                                        "ref": "buttonsTest"
                                    },
                                    "row1_8": {
                                        "type": "question",
                                        "ref": "checkboxTest"
                                    },
                                    "row1_9": {
                                        "type": "question",
                                        "ref": "addressTest"
                                    },
                                    "row1_10": {
                                        "type": "question",
                                        "ref": "distanceTest"
                                    },
                                    "row1_11": {
                                        "type": "question",
                                        "ref": "nameTest"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "submit": {
        "text": "",
        "buttonText": "Beregn",
        "paragraphs": [
            "Gennemgå venligst dine oplysninger før indsendelse.",
            "Sørg for, at alle detaljer er korrekte for at modtage et nøjagtigt tilbud."
        ],
        "submitUrl": "https://httpbin.org/post",
        "submitMethod": "POST",
        "submitFields": {
            "uiSchema": {
                "name": {
                    "ui:placeholder": "Indtast dit navn",
                    "ui:label": "Navn"
                },
                "phone": {
                    "ui:placeholder": "Indtast dit telefonnummer",
                    "ui:label": "Telefon"
                },
                "email": {
                    "ui:placeholder": "Indtast din e-mail",
                    "ui:label": "E-mail"
                }
            },
            "schema": {
                "$schema": "http://json-schema.org/draft-04/schema#",
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "title": "Navn"
                    },
                    "phone": {
                        "type": "string",
                        "format": "tel",
                        "title": "Telefon"
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "title": "E-mail"
                    }
                },
                "required": [
                    "name",
                    "phone",
                    "email"
                ]
            }
        }
    },
    "ending": {
        "inputType": "ending",
        "text": "Tak for at du brugte vores service",
        "paragraph": "Vi vil kontakte dig snarest med dit tilbud."
    },
    "questions": {
        "sliderTest": {
            "defaultValue": 100,
            "inputType": "slider",
            "text": "Slider test",
            "paragraph": "Test af slider",
            "placeholder": "1",
            "min": 1,
            "max": 200,
            "step": 1,
            "type": "number",
            "lang": "DA"
        },
        "checkboxTest": {
            "inputType": "checkbox",
            "text": "Checkbox test",
            "paragraph": "Test af checkbox",
            "type": "boolean",
            "lang": "DA"
        },
        "radioTest": {
            "inputType": "radio",
            "text": "Radio test",
            "paragraph": "Test af radio",
            "options": {
                "yes": "Ja",
                "no": "Nej"
            },
            "lang": "DA",
            "direction": "horizontal"
        },
        "buttonsTest": {
            "inputType": "buttons",
            "text": "Buttons test",
            "paragraph": "Test af buttons",
            "options": {
                "yes": {
                    "key": "yes",
                    "label": "Ja"
                },
                "no": {
                    "key": "no",
                    "label": "Nej"
                },
                "maybe": {
                    "key": "maybe",
                    "label": "Måske"
                },
                "dontKnow": {
                    "key": "dontKnow",
                    "label": "Ved ikke"
                },
                "dontCare": {
                    "key": "dontCare",
                    "label": "Ligeglad"
                },
                "other": {
                    "key": "other",
                    "label": "Andet"
                },
                "other2": {
                    "key": "other2",
                    "label": "Andet 2"
                }
            },
            "lang": "DA"
        },
        "multilineTest": {
            "inputType": "multilinetext",
            "text": "Multilinetext test",
            "paragraph": "Test af multilinetext",
            "lang": "DA"
        },
        "nameTest": {
            "inputType": "text",
            "text": "Text test",
            "paragraph": "Test af text",
            "placeholder": "Skriv noget..",
